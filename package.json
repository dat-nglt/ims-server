{
    "name": "ims-server",
    "version": "1.0.0",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "dev": "nodemon --exec babel-node server.js",
        "start": "node dist/server.js",
        "clean": "rimraf dist",
        "build": "npm run clean && babel src --out-dir dist --copy-files",
        "test:appsecret": "node test-appsecret-proof.js",
        "db:create": "sequelize-cli db:create",
        "db:migrate": "sequelize-cli db:migrate",
        "db:migrate:undo": "sequelize-cli db:migrate:undo",
        "db:seed": "sequelize-cli db:seed:all",
        "db:seed:undo": "sequelize-cli db:seed:undo:all",
        "db:setup": "npm run db:migrate && npm run db:seed"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@google/genai": "^1.29.0",
        "axios": "^1.13.2",
        "bcrypt": "^6.0.0",
        "bcryptjs": "^2.4.3",
        "bullmq": "^5.63.0",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "docker": "^1.0.0",
        "dotenv": "^17.2.3",
        "exceljs": "^4.4.0",
        "express": "^4.18.2",
        "express-rate-limit": "^6.11.2",
        "googleapis": "^166.0.0",
        "helmet": "^7.0.0",
        "joi": "^17.9.2",
        "json2csv": "^6.0.0-alpha.2",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.1",
        "path": "^0.12.7",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.32.1",
        "sequelize-cli": "^6.6.3",
        "socket.io": "^4.8.1",
        "uuid": "^13.0.0",
        "winston": "^3.18.3",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.28.3",
        "@babel/core": "^7.28.5",
        "@babel/node": "^7.28.0",
        "@babel/preset-env": "^7.28.5",
        "nodemon": "^3.1.10",
        "rimraf": "^6.0.1"
    }
}
